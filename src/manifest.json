{

	"manifest_version": 2,
	"name": "Raffa Moreira Mano",
	"version": "1.0",
 	 "description": "Raffa Moreira Mano 77777 !",

	"web_accessible_resources":[
		"777/*.jpg",
		"audios/*.mp3"
	],

	"content_scripts": [

		{

			"matches" : [
				"<all_urls>"
			],

			"js": ["jquery.min.js","content.js"]

		}

	],

	"background": {
		"scripts" :["jquery.min.js","background.js"]
	},
	"browser_action": {
		"default_icon": "faz_sol.png",
		"default_title": "777"
	}
}